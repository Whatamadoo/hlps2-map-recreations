"LightmappedGeneric"
{
	"$basetexture" "halflife/!waterblue"
	"$surfaceprop" "water"
	"%compilewater" 1
	"$abovewater" 1
	"$bottommaterial" "gm_ht05_dorms/waterblue_under"
	$alpha 0.45
	"$angle" 0
	"$translate" 0
	"$scale" "[1 1]"
	"Proxies"
	{
		"Sine"
		{
			"sineperiod" 12
			"sinemin" "1.0"
			"sinemax" "1.025"
			"resultVar" "$scale"
		}
		
		"Sine"
		{
			"sineperiod" 10
			"sinemin" "-.05"
			"sinemax" ".05"
			"resultVar" "$translate"
		}
		
		"Sine"
		{
			"sineperiod" 17
			"sinemin" ".15"
			"sinemax" "-.05"
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"scaleVar" "$scale"
			"resultVar" "$basetexturetransform"
		}
	}
}
